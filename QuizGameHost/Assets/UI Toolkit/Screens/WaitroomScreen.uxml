                </Bindings>
            </ui:Label>
            <ui:Label text="23" style="-unity-font-style: bold;">
                <Bindings>
                    <ui:DataBinding property="text" data-source-path="_connectedUsers" binding-mode="ToTarget" />
                </Bindings>
            </ui:Label>
        </ui:VisualElement>
    </ui:VisualElement>
    <LangButtonElement name="LangButton" style="position: absolute; bottom: 100px; right: 100px;" />
