            </Bindings>
        </ui:Label>
        <ui:VisualElement style="flex-grow: 0; align-items: center; margin-bottom: 30px; margin-top: 60px;">
            <ui:Label text="Room ID">
                <Bindings>
                    <UnityEngine.Localization.LocalizedString property="text" table="GUID:f0a78c96488a0b74ebad8fcd8b85143c" entry="Id(748465290784768)" />
                </Bindings>
            </ui:Label>
            <ui:Label text="123456" name="RoomID" class="red-text" style="-unity-font-style: bold; font-size: 46px;">
                <Bindings>
                    <ui:DataBinding property="text" data-source-path="_roomID" binding-mode="ToTarget" />
