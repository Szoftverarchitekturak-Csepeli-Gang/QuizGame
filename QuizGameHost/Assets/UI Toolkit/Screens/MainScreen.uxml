        <ui:VisualElement name="InputContainer" style="flex-grow: 1; padding-top: 0; padding-right: 30px; padding-bottom: 60px; padding-left: 30px;">
            <ui:TextField label="Username" name="UsernameField">
                <Bindings>
                    <ui:DataBinding property="value" data-source-path="_username" binding-mode="ToSource" />
